{
  "name": "group-circle",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "supervisor ./bin/www",
    "sass-watch": "node-sass -w client/sass/main.scss public/stylesheet/main.min.css",
    "sass-build": "node-sass client/sass/main.scss public/stylesheet/main.min.css --output-style compressed",
    "dev": "concurrently \"node ./bin/www\" \"webpack -d --watch\" \"npm run sass-watch\""
  },
  "dependencies": {
    "concurrently": "^3.5.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "express": "~4.16.0",
    "firebase-admin": "^5.12.1",
    "http-errors": "~1.6.2",
    "jquery": "^3.3.1",
    "messenger-bot": "^2.5.0",
    "morgan": "~1.9.0",
    "node-sass": "^4.7.2",
    "pug": "2.0.0-beta11",
    "supervisor": "^0.12.0",
    "watchify": "^3.10.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8"
  }
}
